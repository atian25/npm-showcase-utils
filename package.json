{
  "name": "npm-showcase-utils",
  "version": "1.0.0",
  "description": "utils for npm-showcase",
  "dependencies": {
  },
  "devDependencies": {
    "autod": "^3",
    "egg-bin": "^4",
    "egg-ci": "^1",
    "eslint": "^7",
    "eslint-config-egg": "^9"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "12, 14",
    "type": "github"
  },
  "eslintIgnore": [ "coverage", "dist" ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "lib",
    "index.js"
  ],
